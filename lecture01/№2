print ("Программа для поиска 3 стороны по 2 другим и углу между ними")
print ("Введите первую сторону а")
a = float (input())
print ("Введите вторую сторону b")
b = float (input())
print ("Задайте угол между стороной а и b в градусах")
Y = int (input())
from math import pi
Y = pi/180*Y
from math import cos
from math import sqrt
K=sqrt(a**2+b**2-2*a*b*cos(Y))
print ("Третья сторона равна", K)
